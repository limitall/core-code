{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@adit/lib/adit": "<rootDir>/../libs/adit/src",
    "@adit/lib/adit/(.*)": "<rootDir>/../libs/adit/src/$1",
    "@adit/core/clickhouse": "<rootDir>/../libs/clickhouse/src",
    "@adit/core/clickhouse/(.*)": "<rootDir>/../libs/clickhouse/src/$1",
    "@adit/core/db": "<rootDir>/../libs/db/src",
    "@adit/core/db/(.*)": "<rootDir>/../libs/db/src/$1",
    "@adit/core/decorators": "<rootDir>/../libs/decorators/src",
    "@adit/core/decorators/(.*)": "<rootDir>/../libs/decorators/src/$1",
    "@adit/core/event": "<rootDir>/../libs/event/src",
    "@adit/core/event/(.*)": "<rootDir>/../libs/event/src/$1",
    "@adit/core/module-registry": "<rootDir>/../libs/module-registry/src",
    "@adit/core/module-registry/(.*)": "<rootDir>/../libs/module-registry/src/$1",
    "@adit/core/postgre": "<rootDir>/../libs/postgre/src",
    "@adit/core/postgre/(.*)": "<rootDir>/../libs/postgre/src/$1",
    "@adit/core/pulsar": "<rootDir>/../libs/pulsar/src",
    "@adit/core/pulsar/(.*)": "<rootDir>/../libs/pulsar/src/$1",
    "@adit/core/util": "<rootDir>/../libs/util/src",
    "@adit/core/util/(.*)": "<rootDir>/../libs/util/src/$1"
  }
}