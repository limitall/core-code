{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@adit/lib/adit": "<rootDir>/../libs/adit/src",
    "@adit/lib/adit/(.*)": "<rootDir>/../libs/adit/src/$1",
    "@limitall/core/decorators": "<rootDir>/../libs/decorators/src",
    "@limitall/core/decorators/(.*)": "<rootDir>/../libs/decorators/src/$1",
    "@limitall/core/event": "<rootDir>/../libs/event/src",
    "@limitall/core/event/(.*)": "<rootDir>/../libs/event/src/$1",
    "@limitall/core/module-registry": "<rootDir>/../libs/module-registry/src",
    "@limitall/core/module-registry/(.*)": "<rootDir>/../libs/module-registry/src/$1",
    "@limitall/core/pulsar": "<rootDir>/../libs/pulsar/src",
    "@limitall/core/pulsar/(.*)": "<rootDir>/../libs/pulsar/src/$1",
    "@limitall/core/util": "<rootDir>/../libs/util/src",
    "@limitall/core/util/(.*)": "<rootDir>/../libs/util/src/$1"
  }
}