{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "assets": [
      "**/*.proto",
      {
        "include": "**/*.proto",
        "outDir": "./dist"
      }
    ],
    "watchAssets": true
  },
  "projects": {
    "adit": {
      "type": "library",
      "root": "libs/adit",
      "entryFile": "index",
      "sourceRoot": "libs/adit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/adit/tsconfig.lib.json"
      }
    },
    "clickhouse": {
      "type": "library",
      "root": "libs/clickhouse",
      "entryFile": "index",
      "sourceRoot": "libs/clickhouse/src",
      "compilerOptions": {
        "tsConfigPath": "libs/clickhouse/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "decorators": {
      "type": "library",
      "root": "libs/decorators",
      "entryFile": "index",
      "sourceRoot": "libs/decorators/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decorators/tsconfig.lib.json"
      }
    },
    "event": {
      "type": "library",
      "root": "libs/event",
      "entryFile": "index",
      "sourceRoot": "libs/event/src",
      "compilerOptions": {
        "tsConfigPath": "libs/event/tsconfig.lib.json"
      }
    },
    "method-registry": {
      "type": "library",
      "root": "libs/method-registry",
      "entryFile": "index",
      "sourceRoot": "libs/method-registry/src",
      "compilerOptions": {
        "tsConfigPath": "libs/method-registry/tsconfig.lib.json"
      }
    },
    "postgre": {
      "type": "library",
      "root": "libs/postgre",
      "entryFile": "index",
      "sourceRoot": "libs/postgre/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgre/tsconfig.lib.json"
      }
    },
    "pulsar": {
      "type": "library",
      "root": "libs/pulsar",
      "entryFile": "index",
      "sourceRoot": "libs/pulsar/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pulsar/tsconfig.lib.json"
      }
    },
    "util": {
      "type": "library",
      "root": "libs/util",
      "entryFile": "index",
      "sourceRoot": "libs/util/src",
      "compilerOptions": {
        "tsConfigPath": "libs/util/tsconfig.lib.json"
      }
    }
  }
}